START
   VARIABLE NUM x; NUM y.

   REPEAT
      STDIN "x? "; x.
   WHILE [ x != 0 ]
   {
      STDIN "y? "; y.
      IF [ x < y ]
      {   
         coutln(" x < y").
      OTHERWISEIF [ x == y ] EXECUTE
         coutln(" x == y").
      OTHERWISEIF [ x > y ] EXECUTE
         coutln(" x > y").
      }
   }
FIN
